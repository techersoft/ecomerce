        // MENU SIDEBAR
        aside.menu-sidebar.d-none.d-lg-block
            .logo
                a(href='#')
                    img(src='images/icon/logo.png', alt='Cool Admin')
            .menu-sidebar__content.js-scrollbar1
                nav.navbar-sidebar
                    ul.list-unstyled.navbar__list
                        li.has-sub
                            a.js-arrow(href='#')
                                i.fas.fa-tachometer-alt
                                | Dashboard
                            ul.list-unstyled.navbar__sub-list.js-sub-list
                                li
                                    a(href='index.html') Dashboard 1
                                li
                                    a(href='index2.html') Dashboard 2
                                li
                                    a(href='index3.html') Dashboard 3
                                li

extends ../layouts/layout-admin.pug
block main-content    
    .page-wrapper
        // HEADER MOBILE
        header.header-mobile.d-block.d-lg-none
            .header-mobile__bar
                .container-fluid
                    .header-mobile-inner
                        a.logo(href='index.html')
                            img(src='images/icon/logo.png', alt='CoolAdmin')
                        button.hamburger.hamburger--slider(type='button')
                            span.hamburger-box
                                span.hamburger-inner
            nav.navbar-mobile
                .container-fluid
                    ul.navbar-mobile__list.list-unstyled
                        li.has-sub
                            a.js-arrow(href='#')
                                i.fas.fa-tachometer-alt
                                | Dashboard
                            ul.navbar-mobile-sub__list.list-unstyled.js-sub-list
                                li
                                    a(href='index.html') Dashboard 1
                                li
                                    a(href='index2.html') Dashboard 2

                                    .au-card-inner
                                        h3.title-2.m-b-40 Doughut Chart
                                        canvas#doughutChart
                            .col-lg-6
                                .au-card.m-b-30
                                    .au-card-inner
                                        h3.title-2.m-b-40 Pie Chart
                                        canvas#pieChart
                            .col-lg-6
                                .au-card.m-b-30
                                    .au-card-inner
                                        h3.title-2.m-b-40 Polar Chart
                                        canvas#polarChart
                            .col-lg-6
                                .au-card.m-b-30
                                    .au-card-inner
